class Person:
  def _int_(self, name, age)
    self.name = name
    self.age = age

p1 = Person("Jhon",36)

print(p1)
#------------------------------------------------------------------------
class Person:
  def _int_(self, name, age)
    self.name = name
    self.age = age

  def_str_(self):
    return f"{self.name}({self.age})"

p1 = Person("Jhon", 36)

print(p1)
